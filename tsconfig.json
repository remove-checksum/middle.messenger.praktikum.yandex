{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2017",
    "declaration": false,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types", "typings/app.d.ts"],
    "resolveJsonModule": true
  }
}
